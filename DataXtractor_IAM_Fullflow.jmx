        <BeanShellSampler guiclass="BeanShellSamplerGui" testclass="BeanShellSampler" testname="Script 1: Writing the validation messages into the excel" enabled="true">
          <stringProp name="BeanShellSampler.query">import org.apache.jmeter.services.FileServer;


try {
	// getting the base directory and creating the excel file
     String path = FileServer.getFileServer().getBaseDir();

     //creating the object to write the data into the file 
     //false is given as 2nd argument to overwrite the existing data 
     f = new FileOutputStream(path+vars.get(&quot;FilePathDEMAIN&quot;), false); 
     p = new PrintStream(f); 
  
     this.interpreter.setOut(p); 
     print(&quot;requestStatus&quot;); //0th row
     print(&quot;Successfully connected&quot;); //1st row
     print(&quot;Only &apos;http&apos; and &apos;https&apos; schemes are allowed&quot;); //2nd row
     print(&quot;No such host is known&quot;); //3rd row
     print(&quot;Database name is incorrect&quot;); //4th row
     print(&quot;Server name is incorrect&quot;);  //5th row   
     print(&quot;Username or password incorrect&quot;); //6th row
     print(&quot;Input Error&quot;); //7th row
     print(&quot;&apos;Name&apos; must not be empty.&quot;); //8th row
     print(&quot;&apos;Description&apos; must not be empty.&quot;); //9th row
     print(&quot;&apos;Host&apos; must not be empty.&quot;); //10th row
     print(&quot;&apos;Data Source Name&apos; must not be empty.&quot;); //11th row
     print(&quot;&apos;Database Name&apos; must not be empty.&quot;);//12th row
     print(&quot;&apos;Server Name&apos; must not be empty.&quot;); //13th row
     print(&quot;&apos;User&apos; must not be empty.&quot;); //14th row
     print(&quot;&apos;Password&apos; must not be empty.&quot;); //15th row
     print(&quot;&apos;ODBC Driver&apos; must not be empty.&quot;); //16th row
     print(&quot;&apos;Name&apos; must not be empty., &apos;Description&apos; must not be empty., &apos;Host&apos; must not be empty., Port must be greater than 0., &apos;Data Source Name&apos; must not be empty., &apos;Database Name&apos; must not be empty., &apos;Server Name&apos; must not be empty., &apos;User&apos; must not be empty., &apos;Password&apos; must not be empty., &apos;ODBC Driver&apos; must not be empty.&quot;); //17th row
     print(&quot;Connection name already exists&quot;); //18th row
     print(&quot;Successfully deleted&quot;); //19th row
     print(&quot;Query name already exists&quot;); //20th row
     print(&quot;&apos;Query String&apos; must not be empty.&quot;); //21st row
     print(&quot;&apos;Name&apos; must not be empty., &apos;Description&apos; must not be empty., &apos;Query String&apos; must not be empty.&quot;); //22nd row
     print(&quot;No queries found&quot;); //23rd row
     print(&quot;LightAgentTxTimeStamp&quot;); //24th row
     print(&quot;DatabaseRxTimeStamp&quot;); //25th row
     print(&quot;DataExtractorSendRequestTimeStamp&quot;); //26th row
     print(&quot;Query string is empty&quot;); //27th row
        
     f.close();

     }
catch (Throwable ex) {
    log.error(&quot;Error in beanshell&quot;, ex);
    throw ex;
}</stringProp>
